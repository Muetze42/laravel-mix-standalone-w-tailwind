  "license": "MIT",
  "homepage": "https://github.com/NormanHuth/laravel-mix-standalone-w-tailwind",
  "dependencies": {
    "tailwindcss": "^1.6.2"
  },
  "devDependencies": {
    "laravel-mix": "^5.0.4",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  }
